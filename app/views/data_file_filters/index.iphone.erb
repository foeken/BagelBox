<p style="padding: 0px 20px 0px 20px;">
  Only manually added filters are shown
</p>
<table class="data_files_table" cellspacing="0" cellpadding="0">
<% @data_file_filters.select{ |x| x.source_id.nil? }.each do |data_file_filter| %>
  <tr class="<%= cycle('even','odd') %>">
    <td nowrap="nowrap">
      <%= link_to data_file_filter.name[0..44], "#", :class => "edit_link" %>
    </td>
  </tr>
<% end %>
</table>